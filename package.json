        "core-js": "^3.15.2",
        "css-loader": "^6.2.0",
        "eslint": "^7.31.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-vue": "^7.14.0",
        "jest": "^27.0.6",
        "less": "^4.1.1",
        "less-loader": "^10.0.1",
        "prettier": "^2.3.2",
        "style-loader": "^3.2.1",
        "ts-jest": "^27.0.4",
        "ts-loader": "^9.2.4",
        "tsconfig-paths-webpack-plugin": "^3.5.1",
        "typescript": "^4.3.5",
