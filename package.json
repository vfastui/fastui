{
    "name": "@vfastui/fastui",
    "version": "0.0.1",
    "description": "vue ui component",
    "main": "./dist/fastui.umd.js",
    "module": "./dist/fastui.es.js",
    "files": [
        "dist","src"
    ],

    "scripts": {
        "doc:dev": "vitepress dev docs --host 0.0.0.0",
        "build": "vitepress build",
        "serve": "vitepress serve",
        "clean": "rimraf lib",
        "build:style": "gulp --gulpfile build/gen-style.js",
        "build:com": "webpack --config build/webpack.component.js",
        "build:prod": "webpack --config build/webpack.prod.js --stats-error-details",
        "dist": "npm run test && npm run build:style && npm run build:prod && npm run build:components",
        "deploy:build": "npm run docs:build && npm run deploy",
        "test": "jest",
        "build:rollup": "vite build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vfastui/fastui.git"
    },
    "author": "airene",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vfastui/fastui/issues"
    },
    "homepage": "https://github.com/vfastui/fastui#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.14.8",
        "@babel/preset-typescript": "^7.14.5",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/vue": "^5.8.1",
        "@types/jest": "^26.0.24",
        "@types/node": "^15.14.2",
        "@typescript-eslint/eslint-plugin": "^4.28.3",
        "@typescript-eslint/parser": "^4.28.3",
        "@vitejs/plugin-vue": "^1.2.5",
        "@vitejs/plugin-vue-jsx": "^1.1.6",
